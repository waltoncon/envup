{
  "name": "@waltoncon/envup",
  "version": "0.0.1",
  "description": "A tool to help initialise and update environment variables for your projects.",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "envup": "bin/envup.js"
  },
  "scripts": {
    "build": "bun build:base --sourcemap=none --minify",
    "dev": "bun build:base --sourcemap=inline",
    "build:base": "bun build src/cli/index.ts --outfile=dist/bin.js --target=node",
    "test": "vitest"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.896.0",
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "globby": "^14.1.0",
    "uuid": "^13.0.0"
  },
  "files": [
    "bin",
    "dist"
  ]
}
